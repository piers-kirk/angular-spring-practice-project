CREATE TABLE Persons
(
PersonID int,
LastName varchar(255),
FirstName varchar(255),
Address varchar(255),
City varchar(255)
);

INSERT INTO dbo.Persons VALUES (1, 'Gerrard', 'Steven', '947 Cornwall Road', 'Liverpool')

CREATE TABLE Shows
(
ShowID int,
ShowName varchar(255),
StreamingPlatform varchar(255),
Genre varchar(255),
);

